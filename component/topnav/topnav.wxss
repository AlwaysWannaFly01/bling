/* component/topnav/topnav.wxss */

.back {
    width: 116px;
    text-align: left;
    padding-left: 30rpx;
    box-sizing: border-box;
}

.ico-back {
    width: 20rpx;
    height: 35rpx;
}

.hh-header {
    position: fixed;
    top: 0;
    width: 100%;
    background-color: #fff;
    z-index: 99;
    text-align: center;
}

.hh-title {
    flex: 1;
    font-size: 38rpx;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.title-bar {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.hh-nav-right {
    width: 116px;
}

.hh-nav-back {
    width: 116px;
}
